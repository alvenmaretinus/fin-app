{
  "extends": ["tslint:recommended", "tslint-react"],
  "rules": {
    "quotemark": [true, "single"],
    "semicolon": [true, "never"],
    "trailing-comma": [true, {"multiline": "always", "singleline": "never"}],
    "interface-name": [true, "never-prefix"],
    "jsx-no-multiline-js": false
  }
}
